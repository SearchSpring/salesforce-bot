{
    "version": 2,
    "env": {
      "SF_URL": "@sf-url",
      "SF_USER":"@sf-user",
      "SF_PASSWORD":"@sf-password",
      "SF_TOKEN":"@sf-token",
      "SLACK_VERIFICATION_TOKEN":"slack-verification-token"
    },
    "builds": [
      {
        "src": "index.go",
        "use": "@now/go"
      }
    ],
    "routes": [
      {
        "src": "(.*)",
        "dest": "index.go"
      }
    ]
  }